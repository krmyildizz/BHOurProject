<!DOCTYPE html>
<html class="fixed" lang="tr">
<head>

    <!-- Basic -->
    <meta charset="UTF-8">

    <title>Admin Panel Giriş</title>
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />

    <!-- Web Fonts  -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800|Shadows+Into+Light" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/Content/Admin/assets/vendor/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/Admin/assets/vendor/font-awesome/css/font-awesome.css" />
    <link rel="stylesheet" href="~/Content/Admin/assets/vendor/magnific-popup/magnific-popup.css" />
    <link rel="stylesheet" href="~/Content/Admin/assets/vendor/bootstrap-datepicker/css/datepicker3.css" />,
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-confirm/3.3.2/jquery-confirm.min.js"></script>
    <!-- Specific Page Vendor CSS -->
    <link rel="stylesheet" href="~/Content/Admin/assets/vendor/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.custom.css" />
    <link rel="stylesheet" href="~/Content/Admin/assets/vendor/bootstrap-multiselect/bootstrap-multiselect.css" />
    <link rel="stylesheet" href="~/Content/Admin/assets/vendor/morris/morris.css" />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/Content/Admin/assets/stylesheets/theme.css" />

    <!-- Skin CSS -->
    <link rel="stylesheet" href="~/Content/Admin/assets/stylesheets/skins/default.css" />

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/Content/Admin/assets/stylesheets/theme-custom.css">

    <!-- Head Libs -->
    <script src="~/Content/Admin/assets/vendor/modernizr/modernizr.js"></script>


    <link href="~/Content/Admin/assets/vendor/bootstrap-toggle/bootstrap-toggle.min.css" rel="stylesheet">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    @RenderSection("Styles", required: false)
</head>
<body>
    <section class="body">

        <!-- start: header -->
        <header class="header">
            <div class="logo-container">
                <a href="/Admin/Home" class="logo">
                    <img src="~/Content/assets/images/logo.png" height="35" alt="Admin Logo" />
                </a>
                <div class="visible-xs toggle-sidebar-left" data-toggle-class="sidebar-left-opened" data-target="html" data-fire-event="sidebar-left-opened">
                    <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                </div>
            </div>

            <!-- start: search & user box -->
            <div class="header-right">
                <span class="separator"></span>

                <div id="userbox" class="userbox">
                    <a role="menuitem" tabindex="-1" href="/Admin/Login/Logout"><i class="fa fa-power-off"></i> Güvenli Çıkış</a>
                </div>
            </div>
            <!-- end: search & user box -->
        </header>
        <!-- end: header -->

        <div class="inner-wrapper">
            <!-- start: sidebar -->
            <aside id="sidebar-left" class="sidebar-left">

                <div class="sidebar-header">
                    <div class="sidebar-title">
                        Menü
                    </div>
                    <div class="sidebar-toggle hidden-xs" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                        <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
                    </div>
                </div>

                <div class="nano">
                    <div class="nano-content">
                        <nav id="menu" class="nav-main" role="navigation">
                            <ul class="nav nav-main">
                                <li class="nav-active">
                                    <a href="/Admin/Product">
                                        <span>Ürünler</span>
                                    </a>
                                </li>
                                <li class="nav-active">
                                    <a href="/Admin/Category">
                                        <span>Kategoriler</span>
                                    </a>
                                </li>
                                <li class="nav-active">
                                    <a href="/Admin/Slider">
                                        <span>Referanslar</span>
                                    </a>
                                </li>
                                <li class="nav-active">
                                    <a href="/Admin/Certificate">
                                        <span>Sertifikalar</span>
                                    </a>
                                </li>
                                <li class="nav-active">
                                    <a href="/Admin/Customer">
                                        <span>Müşteriler</span>
                                    </a>
                                </li>
                            </ul>
                        </nav>
                    </div>

                </div>

            </aside>
            <!-- end: sidebar -->
            <section role="main" class="content-body" ng-app="myAppAdmin">
                @RenderBody()
            </section>
        </div>
    </section>

    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @Scripts.Render("~/bundles/angularAdmin")

    <!-- Vendor -->

    <script src="~/Content/Admin/assets/vendor/jquery/jquery.js"></script>
    <script src="~/Content/Admin/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js"></script>
    <script src="~/Content/Admin/assets/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/Content/Admin/assets/vendor/nanoscroller/nanoscroller.js"></script>
    <script src="~/Content/Admin/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/Content/Admin/assets/vendor/magnific-popup/magnific-popup.js"></script>
    <script src="~/Content/Admin/assets/vendor/jquery-placeholder/jquery.placeholder.js"></script>

    <!-- Specific Page Vendor -->
    <script src="~/Content/Admin/assets/vendor/jquery-ui/js/jquery-ui-1.10.4.custom.js"></script>
    <script src="~/Content/Admin/assets/vendor/jquery-ui-touch-punch/jquery.ui.touch-punch.js"></script>
    <script src="~/Content/Admin/assets/vendor/jquery-appear/jquery.appear.js"></script>
    <script src="~/Content/Admin/assets/vendor/bootstrap-multiselect/bootstrap-multiselect.js"></script>
    <script src="~/Content/Admin/assets/vendor/jquery-easypiechart/jquery.easypiechart.js"></script>
    <script src="~/Content/Admin/assets/vendor/flot/jquery.flot.js"></script>
    <script src="~/Content/Admin/assets/vendor/flot-tooltip/jquery.flot.tooltip.js"></script>
    <script src="~/Content/Admin/assets/vendor/flot/jquery.flot.pie.js"></script>
    <script src="~/Content/Admin/assets/vendor/flot/jquery.flot.categories.js"></script>
    <script src="~/Content/Admin/assets/vendor/flot/jquery.flot.resize.js"></script>
    <script src="~/Content/Admin/assets/vendor/jquery-sparkline/jquery.sparkline.js"></script>
    <script src="~/Content/Admin/assets/vendor/raphael/raphael.js"></script>
    <script src="~/Content/Admin/assets/vendor/morris/morris.js"></script>
    <script src="~/Content/Admin/assets/vendor/gauge/gauge.js"></script>
    <script src="~/Content/Admin/assets/vendor/snap-svg/snap.svg.js"></script>
    <script src="~/Content/Admin/assets/vendor/liquid-meter/liquid.meter.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/jquery.vmap.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/data/jquery.vmap.sampledata.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/maps/jquery.vmap.world.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/maps/continents/jquery.vmap.africa.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/maps/continents/jquery.vmap.asia.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/maps/continents/jquery.vmap.australia.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/maps/continents/jquery.vmap.europe.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/maps/continents/jquery.vmap.north-america.js"></script>
    <script src="~/Content/Admin/assets/vendor/jqvmap/maps/continents/jquery.vmap.south-america.js"></script>
    <script src="~/Content/Admin/assets/vendor/bootstrap-toggle/bootstrap-toggle.min.js"></script>
    <script src="~/Content/Admin/assets/vendor/ios7-switch/ios7-switch.js"></script>
    <!-- Theme Base, Components and Settings -->
    <script src="~/Content/Admin/assets/javascripts/theme.js"></script>

    <!-- Theme Custom -->
    <script src="~/Content/Admin/assets/javascripts/theme.custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="~/Content/Admin/assets/javascripts/theme.init.js"></script>


    <!-- Examples -->
    <script src="~/Content/Admin/assets/javascripts/dashboard/examples.dashboard.js"></script>

    <script src="~/Content/Admin/assets/javascripts/ui-elements/examples.modals.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>